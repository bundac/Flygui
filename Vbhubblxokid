_G.Settings = {
    Main = {
        ["Farm Mode Position"] = 2,
        ["Farm Mode"] = 1,
        ["Enable Auto Farm"] = false,
        ["Enable WaterWalk"] = true,
        ["Saved Position Coordinat"] = nil,
        ["Select Fruit"] = 1,
        ["Select Island"] = 1,
        ["Select NPC"] = 1,
        ["Select Boss"] = 1,
        ["Sell Wait"] = 20
    }
}
function LoadSettings()
    if readfile and (writefile and (isfile and isfolder)) then
        if not isfolder("Vietbun Hub") then
            makefolder("Vietbun Hub")
        end
        if not isfolder("Vietbun Hub/Blox Fruit/") then
            makefolder("Vietbun Hub/Blox Fruit/")
        end
        if isfile("Vietbun Hub/Blox Fruit/" .. game.Players.LocalPlayer.Name .. ".json") then
            local v3_ = game:GetService("HttpService"):JSONDecode(readfile("Vietbun Hub/Blox Fruit/" .. game.Players.LocalPlayer.Name .. ".json"))
            local v4_, v5_, v6_ = pairs(v3_)
            while true do
                local v7_
                v6_, v7_ = v4_(v5_, v6_)
                if v6_ == nil then
                    break
                end
                _G.Settings[v6_] = v7_
            end
        else
            writefile("Vietbun Hub/Blox Fruit/" .. game.Players.LocalPlayer.Name .. ".json", game:GetService("HttpService"):JSONEncode(_G.Settings))
        end
    else
        local _ = warn
    end
end
function SaveSettings()
    if readfile and (writefile and (isfile and isfolder)) then
        if isfile("Vietbun Hub/Blox Fruit/" .. game.Players.LocalPlayer.Name .. ".json") then
            game:GetService("HttpService"):JSONDecode(readfile("Vietbun Hub/Blox Fruit/" .. game.Players.LocalPlayer.Name .. ".json"))
            local v8_, v9_, v10_ = pairs(_G.Settings)
            local v11_ = {}
            while true do
                local v12_
                v10_, v12_ = v8_(v9_, v10_)
                if v10_ == nil then
                    break
                end
                v11_[v10_] = v12_
            end
            writefile("Vietbun Hub/Blox Fruit/" .. game.Players.LocalPlayer.Name .. ".json", game:GetService("HttpService"):JSONEncode(v11_))
        else
            LoadSettings()
        end
    else
        local _ = warn
    end
end
LoadSettings()
