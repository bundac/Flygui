spawn(function()
    while task.wait() do
        pcall(function()
            if game:GetService("Workspace").Map:FindFirstChild("HellIsland") then
                Raid:SetDesc("Active")
            else
                Raid:SetDesc("Not Active")
            end
            if game:GetService("Workspace").Map:FindFirstChild("CursedShip") then
                CursedShip:SetDesc("Spawned")
            else
                CursedShip:SetDesc("Not Spawned")
            end
            if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyBlackLeg",true) then
                FightingStyle:SetDesc("Available")
            else
                FightingStyle:SetDesc("Not Available")
            end
        end)
    end
end)
