    while true do
        game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("afk"):FireServer(false)
        task.wait(0.01)
    end
end)
spawn(function()
    pcall(function()
        game:GetService("RunService").Stepped:Connect(function()
            if _G.Settings.Main["Enable Auto Farm"] or _G.Settings.Main.BastiaryEvent then
                if not game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
                    local v19_ = Instance.new("BodyVelocity")
                    v19_.Name = "BodyClip"
                    v19_.Parent = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart
                    v19_.MaxForce = Vector3.new(100000, 100000, 100000)
                    v19_.Velocity = Vector3.new(0, 0, 0)
                end
            elseif game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip"):Destroy()
            end
        end)
    end)
end)
local v_u_20_ = "Instant"
local v_u_21_ = 100
local v_u_22_ = 1
local v_u_23_ = 20
function TeleportPlayer(p24_)
    local v25_ = game.Players.LocalPlayer
    local v26_ = (v25_.Character or v25_.CharacterAdded:Wait()):FindFirstChild("HumanoidRootPart")
    if v26_ then
local v27_ = p24_.Position
        if v_u_20_ == "Instant" then
            v26_.CFrame = CFrame.new(v27_)
            return
        end
        while v_u_22_ < (v26_.Position - v27_).Magnitude do
            local v28_ = v26_.Position
            local v29_ = (v27_ - v28_).Magnitude
            local v30_ = v29_ < v_u_23_ and math.clamp(v29_ * 0.5, 5, v_u_21_) or v_u_21_
            local v31_ = v28_ + (v27_ - v28_).Unit * v30_ * game:GetService("RunService").Heartbeat:Wait()
            v26_.CFrame = CFrame.new(v31_)
        end
    end
end
