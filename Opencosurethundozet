    Title = "Buy Relic C$ 11,000",
    Description = "",
    Callback = function()
        local v304_ = workspace:FindFirstChild("world") and (workspace.world:FindFirstChild("npcs") and workspace.world.npcs:FindFirstChild("Merlin"))
        if v304_ then
            v304_ = workspace.world.npcs.Merlin:FindFirstChild("Merlin")
        end
        if v304_ then
            workspace:WaitForChild("world"):WaitForChild("npcs"):WaitForChild("Merlin"):WaitForChild("Merlin"):WaitForChild("power"):InvokeServer()
        else
            TeleportPlayer(CFrame.new(- 932, 224, - 988))
            repeat
                task.wait()
            until v304_
            TeleportPlayer(currentCFrame)
        end
    end
})
