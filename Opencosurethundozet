    Multi = false,
    Default = nil
})
local v78_ = v_u_77_
v_u_77_.OnChanged(v78_, function(p79_)
    SelectedWorldEvent = p79_
    if SelectedWorldEvent ~= "Strange Whirlpool" then
if SelectedWorldEvent ~= "Great Hammerhead Shark" then
            if SelectedWorldEvent ~= "Great White Shark" then
                if SelectedWorldEvent ~= "Whale Shark" then
                    if SelectedWorldEvent ~= "The Depths - Serpent" then
                        if SelectedWorldEvent == "Megalodon" then
                            local v80_ = Vector3.new(0, 50, 0)
                            local v81_, v82_, v83_ = pairs(game.Workspace.zones.fishing:GetChildren())
                            local v84_ = {}
                            while true do
                                local v85_
                                v83_, v85_ = v81_(v82_, v83_)
                                if v83_ == nil then
                                    break
                                end
                                if string.find(v85_.Name, "Megalodon") then
                                    table.insert(v84_, v85_)
                                end
                            end
                            if # v84_ <= 0 then
                                v_u_77_:SetValue(nil)
                                local _ = v_u_57_.Notify
                            else
                                local v86_, v87_, v88_ = ipairs(v84_)
                                while true do
                                    local v89_
                                    v88_, v89_ = v86_(v87_, v88_)
                                    if v88_ == nil then
                                        break
                                    end
                                    print("Found Megalodon-like object: " .. v89_.Name)
                                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v89_.Position + v80_)
                                end
                            end
                        end
                    else
                        local v90_ = Vector3.new(0, 50, 0)
                        if not game.Workspace.zones.fishing:FindFirstChild("The Depths - Serpent") then
                            v_u_77_:SetValue(nil)
                            return v_u_57_:Notify({
                                Title = "Thunder Z Hub",
                                Content = "The Depths - Serpent not spawn yet ",
                                Duration = 9
                            })
                        end
                        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.zones.fishing["The Depths - Serpent"].Position + v90_)
                        v_u_77_:SetValue(nil)
                    end
                else
                    local v91_ = Vector3.new(0, 135, 0)
                    if not game.Workspace.zones.fishing:FindFirstChild("Whale Shark") then
                        v_u_77_:SetValue(nil)
                        return v_u_57_:Notify({
                            Title = "Thunder Z Hub",
                            Content = "Whale Shark not spawn yet",
                            Duration = 9
                        })
                    end
                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.zones.fishing["Whale Shark"].Position + v91_)
                    v_u_77_:SetValue(nil)
                end
            else
                local v92_ = Vector3.new(0, 135, 0)
                if not game.Workspace.zones.fishing:FindFirstChild("Great White Shark") then
                    v_u_77_:SetValue(nil)
                    return v_u_57_:Notify({
                        Title = "Thunder Z Hub",
                        Content = "Great White Shark not spawn yet",
                        Duration = 9
                    })
                end
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.zones.fishing["Great White Shark"].Position + v92_)
                v_u_77_:SetValue(nil)
            end
        else
            local v93_ = Vector3.new(0, 135, 0)
            if not game.Workspace.zones.fishing:FindFirstChild("Great Hammerhead Shark") then
                v_u_77_:SetValue(nil)
                return v_u_57_:Notify({
                    Title = "Thunder Z Hub",
                    Content = "Great Hammerhead Shark not spawn yet",
                    Duration = 9
                })
            end
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.zones.fishing["Great Hammerhead Shark"].Position + v93_)
            v_u_77_:SetValue(nil)
        end
    else
        local v94_ = Vector3.new(25, 135, 25)
        if not game.Workspace.zones.fishing:FindFirstChild("Isonade") then
            v_u_77_:SetValue(nil)
            return v_u_57_:Notify({
                Title = "Thunder Z Hub",
                Content = "Strange Whirlpool not spawn yet",
                Duration = 9
            })
        end
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.zones.fishing.Isonade.Position + v94_)
        v_u_77_:SetValue(nil)
    end
end)
