function LoadSettings()
    if readfile and (writefile and (isfile and isfolder)) then
        if not isfolder("ThunderZ Hub") then
            makefolder("ThunderZ Hub")
        end
        if not isfolder("ThunderZ Hub/Fisch/") then
            makefolder("ThunderZ Hub/Fisch/")
        end
        if isfile("ThunderZ Hub/Fisch/" .. game.Players.LocalPlayer.Name .. ".json") then
            local v3_ = game:GetService("HttpService"):JSONDecode(readfile("ThunderZ Hub/Fisch/" .. game.Players.LocalPlayer.Name .. ".json"))
            local v4_, v5_, v6_ = pairs(v3_)
            while true do
                local v7_
                v6_, v7_ = v4_(v5_, v6_)
                if v6_ == nil then
                    break
                end
                _G.Settings[v6_] = v7_
            end
        else
            writefile("ThunderZ Hub/Fisch/" .. game.Players.LocalPlayer.Name .. ".json", game:GetService("HttpService"):JSONEncode(_G.Settings))
        end
    else
        local _ = warn
    end
end
function SaveSettings()
    if readfile and (writefile and (isfile and isfolder)) then
        if isfile("ThunderZ Hub/Fisch/" .. game.Players.LocalPlayer.Name .. ".json") then
            game:GetService("HttpService"):JSONDecode(readfile("ThunderZ Hub/Fisch/" .. game.Players.LocalPlayer.Name .. ".json"))
            local v8_, v9_, v10_ = pairs(_G.Settings)
            local v11_ = {}
            while true do
                local v12_
                v10_, v12_ = v8_(v9_, v10_)
                if v10_ == nil then
                    break
                end
                v11_[v10_] = v12_
            end
            writefile("ThunderZ Hub/Fisch/" .. game.Players.LocalPlayer.Name .. ".json", game:GetService("HttpService"):JSONEncode(v11_))
        else
            LoadSettings()
        end
    else
        local _ = warn
    end
end
LoadSettings()
