        return loadstring("return CFrame.new(" .. _G.Settings.Main["Saved Position Coordinat"] .. ")")()
    end)
    if v66_ and typeof(v67_) == "CFrame" then
        v65_ = string.format("%d, %d, %d", math.floor(v67_.Position.X + 0.5), math.floor(v67_.Position.Y + 0.5), math.floor(v67_.Position.Z + 0.5))
