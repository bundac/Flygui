    Callback = function()
        local v68_ = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if v68_ then
            _G.Settings.Main["Saved Position Coordinat"] = tostring(v68_.CFrame)
            local v69_ = v68_.Position
            local v70_ = string.format("%d, %d, %d", math.floor(v69_.X + 0.5), math.floor(v69_.Y + 0.5), math.floor(v69_.Z + 0.5))
            PosisiSekarang:SetDesc(v70_)
            setclipboard(tostring(v68_.CFrame))
            SaveSettings()
        else
            warn("HumanoidRootPart tidak ditemukan!")
        end
    end
})
