function getAllOwnedFish()
    local v212_, v213_, v214_ = ipairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren())
    local v215_ = {}
    while true do
        local v216_
        v214_, v216_ = v212_(v213_, v214_)
        if v214_ == nil then
            break
        end
        if v216_:FindFirstChild("fishscript") then
            v215_[v216_.Name] = true
        end
    end
    local v217_, v218_, v219_ = pairs(v215_)
    local v220_ = {}
    while true do
        v219_ = v217_(v218_, v219_)
        if v219_ == nil then
            break
        end
        table.insert(v220_, v219_)
    end
    return v220_
end
local v221_ = getAllOwnedFish()
