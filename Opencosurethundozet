local v71_ = v61_.Main:AddSection("[ World Event ]")
Megalodon = v71_:AddParagraph({
    Title = "Megalodon Status",
    Content = ""
})
Serpent = v71_:AddParagraph({
    Title = "The Depths - Serpent Status",
    Content = ""
})
GreatHammerhead = v71_:AddParagraph({
    Title = "Great Hammerhead Shark",
    Content = ""
})
GreatWhite = v71_:AddParagraph({
    Title = "Great White Shark",
    Content = ""
})
WhaleShark = v71_:AddParagraph({
    Title = "Whale Shark Status",
    Content = ""
})
StrangeWhirlpool = v71_:AddParagraph({
    Title = "Strange Whirlpool Status",
    Content = ""
})
spawn(function()
    while task.wait() do
        pcall(function()
            local v72_, v73_, v74_ = pairs(game.Workspace.zones.fishing:GetChildren())
            local v75_ = {}
            while true do
                local v76_
                v74_, v76_ = v72_(v73_, v74_)
                if v74_ == nil then
                    break
                end
                if string.find(v76_.Name, "Megalodon") then
                    table.insert(v75_, v76_)
                end
            end
            if # v75_ <= 0 then
                Megalodon:SetDesc("Not Spawn")
            else
                Megalodon:SetDesc("Spawn")
            end
            if game.Workspace.zones.fishing:FindFirstChild("The Depths - Serpent") then
                Serpent:SetDesc("Spawn")
            else
                Serpent:SetDesc("Not Spawn")
            end
            if game.Workspace.zones.fishing:FindFirstChild("Great Hammerhead Shark") then
                GreatHammerhead:SetDesc("Spawn")
            else
                GreatHammerhead:SetDesc("Not Spawn")
            end
            if game.Workspace.zones.fishing:FindFirstChild("Great White Shark") then
                GreatWhite:SetDesc("Spawn")
            else
                GreatWhite:SetDesc("Not Spawn")
            end
            if game.Workspace.zones.fishing:FindFirstChild("Whale Shark") then
                WhaleShark:SetDesc("Spawn")
            else
                WhaleShark:SetDesc("Not Spawn")
            end
            if game.Workspace.zones.fishing:FindFirstChild("Isonade") then
                StrangeWhirlpool:SetDesc("Spawn")
            else
                StrangeWhirlpool:SetDesc("Not Spawn")
            end
        end)
    end
end)
local v_u_77_ = v71_:AddDropdown("WorldEventTPDropdownUI", {
    Title = "Select World Event",
    Description = "You Need Current Position to Auto Fish here",
    Values = {
        "Megalodon",
        "Strange Whirlpool",
