slebew = false
function Mewing()
    if v_u_105_() then
        local v114_ = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("events")
        if v114_ then
            v114_ = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").events:FindFirstChild("cast")
        end
        if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("events") then
            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").events:FindFirstChild("reset")
        end
        local v115_ = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("shakeui")
        local v116_ = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("reel")
        if v114_ and not (v115_ or v116_) then
            if _G.Settings.Main["Farm Mode"] == "Fast" or _G.Settings.Main["Farm Mode"] == "Normal" then
                game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").events.cast:FireServer(100)
            elseif _G.Settings.Main["Farm Mode"] == "Legit" then
                game:GetService("VirtualInputManager"):SendMouseButtonEvent(55, 208, 0, true, game.Players.LocalPlayer.PlayerGui, 1)
                wait(math.random(1, 1.1))
                game:GetService("VirtualInputManager"):SendMouseButtonEvent(55, 208, 0, false, game.Players.LocalPlayer.PlayerGui, 1)
            end
        elseif v115_ then
            if _G.Settings.Main["Farm Mode"] == "Fast" or _G.Settings.Main["Farm Mode"] == "Normal" then
                if game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button then
                    game:GetService("GuiService").SelectedObject = game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button
                    game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.Visible = false
                else
                    game:GetService("GuiService").SelectedObject = nil
                end
                if game:GetService("GuiService").SelectedObject ~= game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button then
                    return
                end
                game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.Visible = false
                game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Return, false, game)
                game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Return, false, game)
            elseif _G.Settings.Main["Farm Mode"] == "Legit" then
                if game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button then
                    game:GetService("GuiService").SelectedObject = game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button
                else
                    game:GetService("GuiService").SelectedObject = nil
                end
                if game:GetService("GuiService").SelectedObject ~= game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button then
                    return
                end
                game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Return, false, game)
                task.wait(0.2)
                game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Return, false, game)
            end
        elseif v116_ then
            if _G.Settings.Main["Farm Mode"] ~= "Fast" then
                if _G.Settings.Main["Farm Mode"] ~= "Normal" then
                    if _G.Settings.Main["Farm Mode"] == "Legit" then
                        repeat
                            task.wait(0.25)
                            game:GetService("Players").LocalPlayer.PlayerGui.reel.bar:FindFirstChild("playerbar").Position = game:GetService("Players").LocalPlayer.PlayerGui.reel.bar:FindFirstChild("fish").Position
                        until not v116_
                    end
                else
                    game:GetService("Players").LocalPlayer.PlayerGui.reel.bar:FindFirstChild("playerbar").Position = game:GetService("Players").LocalPlayer.PlayerGui.reel.bar:FindFirstChild("fish").Position
                end
            else
                game:GetService("ReplicatedStorage").events:FindFirstChild("reelfinished"):FireServer(100, true)
            end
        end
    else
        v_u_111_()
    end
end
spawn(function()
    while task.wait() do
        if _G.Settings.Main["Enable Auto Farm"] then
            pcall(function()
                if _G.Settings.Main["Farm Mode Position"] ~= "Safe Position" then
                    if _G.Settings.Main["Farm Mode Position"] ~= "Current Position" then
                        if _G.Settings.Main["Farm Mode Position"] == "Saved Position" then
                            if _G.Settings.Main["Saved Position Coordinat"] then
                                local v117_, v118_ = pcall(function()
                                    return loadstring("return CFrame.new(" .. _G.Settings.Main["Saved Position Coordinat"] .. ")")()
                                end)
                                if v117_ and typeof(v118_) == "CFrame" then
                                    local v119_ = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                                    if v119_ then
                                        if (v118_.Position - v119_.Position).magnitude > 10 then
                                            v119_.CFrame = v118_
                                        else
                                            Mewing()
                                        end
                                    else
                                        warn("HumanoidRootPart tidak ditemukan!")
                                    end
                                else
                                    warn("Gagal memuat CFrame yang valid! Periksa format string.")
                                end
                            else
                                v_u_57_:Notify({
                                    Title = "Thunder Z Hub",
                                    Content = "Save Your Position First",
                                    Duration = 9
                                })
                            end
                        end
                    else
                        Mewing()
                    end
                elseif (CFrame.new(1889, 130, - 3771).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 5 then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").SafePosition.CFrame * CFrame.new(0, 4, 0)
                else
                    Mewing()
                end
            end)
        end
    end
end)
