    Title = "Infinite Range NPC Interaction",
    Description = "Use this to access NPC functions anywhere for Appraise, Merchant, Merlin, and Jack Marrow.",
    Callback = function()
        local v175_ = {
            Merlin = CFrame.new(- 932, 224, - 988),
            ["Jack Marrow"] = CFrame.new(- 2825, 214, 1516),
            Appraiser = CFrame.new(444, 151, 210),
            ["Marc Merchant"] = CFrame.new(467, 151, 231)
        }
        local v176_ = game.Players.LocalPlayer
        local v_u_177_ = (v176_.Character or v176_.CharacterAdded:Wait()):WaitForChild("HumanoidRootPart")
        local v178_ = v_u_177_.CFrame
        local function v186_(p179_)
            local v180_ = workspace.CurrentCamera
            local _ = v180_.CameraType
            local _ = v180_.CFrame
            local v181_
            if p179_.Parent:IsA("Model") and p179_.Parent:FindFirstChild("Head") then
                v181_ = p179_.Parent.Head
            else
                if not p179_.Parent:IsA("Part") then
                    return
                end
                v181_ = p179_.Parent
            end
            local v182_ = p179_.Enabled
            local v183_ = p179_.HoldDuration
            local v184_ = p179_.RequiresLineOfSight
            local v185_ = p179_.MaxActivationDistance
            p179_.Enabled = true
            p179_.MaxActivationDistance = math.huge
            p179_.RequiresLineOfSight = false
            p179_.HoldDuration = 0
            v180_.CameraType = Enum.CameraType.Scriptable
            v180_.CFrame = CFrame.lookAt(v180_.CFrame.Position, v181_.Position)
            task.wait(0.05)
            p179_:InputHoldBegin()
            task.wait(0.05)
            p179_:InputHoldEnd()
            task.wait(0.5)
            v180_.CameraType = Enum.CameraType.Custom
            v180_.CFrame = CFrame.new(v_u_177_.Position)
            p179_.Enabled = v182_
            p179_.HoldDuration = v183_
            p179_.RequiresLineOfSight = v184_
            p179_.MaxActivationDistance = v185_
        end
        local v187_, v188_, v189_ = pairs(v175_)
        local v190_ = {}
        local function v192_(p191_)
            if p191_ then
                p191_ = p191_.ModelStreamingMode == Enum.ModelStreamingMode.Persistent
            end
            return p191_
        end
        while true do
            v189_ = v187_(v188_, v189_)
            if v189_ == nil then
                break
            end
            v190_[v189_] = false
        end
        local v193_, v194_, v195_ = pairs(v175_)
        while true do
            local v196_
            v195_, v196_ = v193_(v194_, v195_)
            if v195_ == nil then
                break
            end
            TeleportPlayer(v196_)
            task.wait(2)
            local v197_ = workspace.world.npcs:FindFirstChild(v195_)
            if v197_ then
                v197_.ModelStreamingMode = Enum.ModelStreamingMode.Persistent
                if v192_(v197_) then
                    local v198_ = v197_:FindFirstChildWhichIsA("ProximityPrompt", true)
                    if v198_ then
                        v186_(v198_)
                        v190_[v195_] = true
                    end
                else
                    task.wait(2)
                end
            else
                warn(v195_ .. " not found in the workspace.world.npcs after teleportation.")
            end
        end
        local v199_, v200_, v201_ = pairs(v190_)
        local v202_ = true
        while true do
            local v203_
            v201_, v203_ = v199_(v200_, v201_)
            if v201_ == nil then
                break
            end
            if not v203_ then
                v202_ = false
                break
            end
        end
        if v202_ then
            v_u_57_:Notify({
                Title = "Thunder Z Hub",
                Content = "NPCs can now be accessed from an unlimited range for various interactions, such as Buying Luck/Relics, Selling Fish, Appraising Items, and Repairing Maps.",
                Duration = math.huge
            })
        end
        task.wait(2)
        TeleportPlayer(v178_)
    end
})
local function v_u_208_()
    local v204_, v205_, v206_ = ipairs(workspace.world.npcs:GetChildren())
    while true do
        local v207_
        v206_, v207_ = v204_(v205_, v206_)
        if v206_ == nil then
            break
        end
        if v207_.Name:find("Merchant") and v207_:FindFirstChild("merchant") then
            return v207_
        end
    end
    return nil
end
