function AlphaWolf()
    if v_u_105_() then
        local v140_ = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("events")
        if v140_ then
            v140_ = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").events:FindFirstChild("cast")
        end
        if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool"):FindFirstChild("events") then
            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").events:FindFirstChild("reset")
        end
        local v141_ = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("shakeui")
        local v142_ = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("reel")
        if v140_ and not (v141_ or v142_) then
            game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").events.cast:FireServer(100)
        elseif v141_ then
            if game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button then
                game:GetService("GuiService").SelectedObject = game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button
                game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.Visible = false
            else
                game:GetService("GuiService").SelectedObject = nil
            end
            if game:GetService("GuiService").SelectedObject ~= game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.button then
                return
            end
game:GetService("Players").LocalPlayer.PlayerGui.shakeui.safezone.Visible = false
            game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Return, false, game)
            game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Return, false, game)
        elseif v142_ then
            game:GetService("ReplicatedStorage").events:FindFirstChild("reelfinished"):FireServer(100, true)
        end
    else
        v_u_111_()
    end
end
v139_:AddToggle("RodDepth", {
    Title = "Complete The Depths Puzzle",
    Description = "it Will Doing Full Puzzle Quest & Buy the Rod of The Depths",
    Default = _G.Settings.Main.TheDepthQuest
})
v_u_62_.RodDepth:OnChanged(function()
    _G.Settings.Main.TheDepthQuest = v_u_62_.RodDepth.Value
end)
spawn(function()
    while task.wait() do
        if _G.Settings.Main.TheDepthQuest then
            pcall(function()
                if game:GetService("ReplicatedStorage").playerstats[game.Players.LocalPlayer.Name].Rods:FindFirstChild("Rod Of The Depths") then
                    PancinganProgress:SetDesc("You Already Have Rod Of The Depths")
                elseif game:GetService("ReplicatedStorage").playerstats[game.Players.LocalPlayer.Name].Cache:FindFirstChild("Door.TheDepthsGate") then
                    local v143_ = game:GetService("ReplicatedStorage").playerstats[game.Players.LocalPlayer.Name].Inventory
                    local v144_, v145_, v146_ = ipairs(v143_:GetChildren())
                    local v147_ = false
                    local v148_ = false
                    local v149_ = false
                    while true do
                        local v150_
                        v146_, v150_ = v144_(v145_, v146_)
                        if v146_ == nil then
                            break
                        end
                        if string.find(v150_.Name, "Enchant Relic") then
                            v149_ = true
                            if v150_:FindFirstChild("Mutation") then
                                local v151_ = v150_.Mutation.Value
                                v147_ = v147_ or v151_ == "Hexed"
                                if not v148_ then
                                    if v151_ == "Abyssal" then
                                        v148_ = true
                                    else
                                        v148_ = false
                                    end
                                end
                            end
                        end
                    end
                    if v149_ then
                        if v147_ and v148_ then
                            if game:GetService("Players").LocalPlayer.leaderstats["C$"].Value < "750,000C$" then
                                PancinganProgress:SetDesc("You Coins is Less than 750,000 to Buy Rod of The Depths")
                            else
                                PancinganProgress:SetDesc("Doing a Puzzle and Buying the Relic")
                                local function v_u_163_(p152_)
                                    local v153_, v154_, v155_ = ipairs(game:GetService("ReplicatedStorage").playerstats[game.Players.LocalPlayer.Name].Inventory:GetChildren())
                                    while true do
                                        local v156_
                                        v155_, v156_ = v153_(v154_, v155_)
                                        if v155_ == nil then
                                            break
                                        end
                                        if string.find(v156_.Name, "Enchant Relic") and (v156_:FindFirstChild("Mutation") and v156_.Mutation.Value == p152_) then
                                            local v157_ = v156_.Name
                                            local v158_, v159_, v160_ = ipairs(game.Players.LocalPlayer.Backpack:GetChildren())
                                            while true do
                                                local v161_
                                                v160_, v161_ = v158_(v159_, v160_)
                                                if v160_ == nil then
                                                    break
                                                end
                                                if v161_:FindFirstChild("link") then
                                                    local v162_ = tostring(v161_.link.Value):match("^%s*(.-)%s*$")
                                                    if string.find(v161_.Name, "Enchant Relic") and v162_ == v157_ then
                                                        v161_.Name = v162_
                                                        return v161_.Name
                                                    end
                                                end
                                            end
                                        end
                                    end
                                    return nil
                                end
                                local function v_u_166_(p_u_164_)
                                    pcall(function()
                                        local v165_ = game.Players.LocalPlayer.Backpack:FindFirstChild(p_u_164_)
                                        if v165_ then
                                            game.Players.LocalPlayer.Character.Humanoid:EquipTool(v165_)
                                        else
                                            print("Relic tidak ditemukan di Backpack.")
                                        end
                                    end)
                                end
                                local function v167_()
                                    AbyssalTarok = false
                                    HexedTarok = false
                                    TPDepth = false
                                    BuyRodDepth = false
                                    while not (BuyRodDepth and (TPDepth and (AbyssalTarok and HexedTarok))) do
                                        if TPDepth then
                                            if TPDepth and not AbyssalTarok then
                                                if (CFrame.new(1205.68042, - 716.498291, 1314.95325).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 5 then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1205.68042, - 716.498291, 1314.95325, - 0.39891386, - 7.99506026e-8, - 0.916988373, - 2.28649921e-9, 1, - 8.61935519e-8, 0.916988373, - 3.22871081e-8, - 0.39891386)
                                                else
                                                    v_u_166_(v_u_163_("Abyssal"))
                                                    wait(2)
                                                    game:GetService("ReplicatedStorage").packages.Net["RE/ActivatorClientActive"]:FireServer("Abyssal")
                                                    wait(3)
                                                    AbyssalTarok = true
                                                end
                                            elseif TPDepth and (AbyssalTarok and not HexedTarok) then
                                                if (CFrame.new(1049.36633, - 632.940063, 1319.16992).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 5 then
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1049.36633, - 632.940063, 1319.16992, 0.824049056, 4.00164435e-9, - 0.566518486, - 5.11433633e-8, 1, - 6.73287701e-8, 0.566518486, 8.44558699e-8, 0.824049056)
                                                else
                                                    v_u_166_(v_u_163_("Hexed"))
                                                    wait(2)
                                                    game:GetService("ReplicatedStorage").packages.Net["RE/ActivatorClientActive"]:FireServer("Hexed")
                                                    wait(3)
                                                    HexedTarok = true
                                                end
                                            elseif not BuyRodDepth and (TPDepth and (AbyssalTarok and HexedTarok)) then
                                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1703, - 903, 1443)
                                                if game:GetService("Players").LocalPlayer.PlayerGui.ProximityPrompts:FindFirstChild("PromptTemplate") then
                                                    game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.E, false, game)
                                                    game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.E, false, game)
                                                elseif game:GetService("Players").LocalPlayer.PlayerGui.over:FindFirstChild("prompt") then
                                                    if game:GetService("Players").LocalPlayer.PlayerGui.over.prompt.confirm then
                                                        game:GetService("GuiService").SelectedObject = game:GetService("Players").LocalPlayer.PlayerGui.over.prompt.confirm
                                                    else
                                                        game:GetService("GuiService").SelectedObject = nil
                                                    end
                                                    if game:GetService("GuiService").SelectedObject ~= game:GetService("Players").LocalPlayer.PlayerGui.over.prompt.confirm then
                                                        return
                                                    end
                                                    game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Return, false, game)
                                                    game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Return, false, game)
                                                end
                                            end
                                        elseif (CFrame.new(946, - 712, 1254).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude > 5 then
                                            wait(10)
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(946, - 712, 1254)
                                            wait(10)
                                        else
                                            wait(2)
                                            TPDepth = true
                                        end
                                    end
                                end
                                if v_u_163_("Hexed") or v_u_163_("Abyssal") then
                                    v167_()
                                end
                            end
                        elseif v147_ then
                            PancinganProgress:SetDesc("you dont have Abyssal Relic please use auto praiser at NPC section")
                        elseif v148_ then
                            PancinganProgress:SetDesc("you dont have Hexed Relic please use auto praiser at NPC section")
                        else
                            PancinganProgress:SetDesc("you dont have Abyssal&Hexed Relic please use auto praiser at NPC section")
                        end
                    else
                        local v168_ = workspace:FindFirstChild("world") and (workspace.world:FindFirstChild("npcs") and workspace.world.npcs:FindFirstChild("Merlin"))
                        if v168_ then
                            v168_ = workspace.world.npcs.Merlin:FindFirstChild("Merlin")
                        end
                        if v168_ then
                            wait(1)
                            workspace:WaitForChild("world"):WaitForChild("npcs"):WaitForChild("Merlin"):WaitForChild("Merlin"):WaitForChild("power"):InvokeServer()
                        else
