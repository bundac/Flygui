local v284_ = v61_.NPC:AddSection("[ Jack Marrow ]")
local v_u_285_ = game:GetService("Players").LocalPlayer
local v286_ = v_u_285_
v_u_285_.WaitForChild(v286_, "Backpack")
v284_:AddToggle("AutoTreasure", {
    Title = "Auto Find Treasure",
    Default = _G.Treasure
})
v_u_62_.AutoTreasure:OnChanged(function()
    _G.Treasure = v_u_62_.AutoTreasure.Value
end)
spawn(function()
    while task.wait() do
        if _G.Treasure then
            pcall(function()
                if game.Players.LocalPlayer.Backpack:FindFirstChild("Treasure Map") then
                    if EquipTreasureMap() then
                        RepairTreasureMap()
                        TeleportToTreasure()
                        ActivateNearestChestPrompt()
                    end
                    wait(1)
                end
            end)
        end
    end
end)
local v_u_287_ = false
function EquipTreasureMap()
    local v288_ = game.Players.LocalPlayer.Backpack:FindFirstChild("Treasure Map")
    if v288_ then
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v288_)
        return true
    else
        warn("No Treasure Map found in backpack.")
        return false
    end
end
function RepairTreasureMap()
    if not v_u_287_ then
        local v289_ = CFrame.new(- 2825, 214, 1516)
        TeleportPlayer(v289_)
        v_u_287_ = true
    end
    local v290_ = workspace.world.npcs:FindFirstChild("Jack Marrow")
    while not v290_ do
        wait(0.5)
        v290_ = workspace.world.npcs:FindFirstChild("Jack Marrow")
    end
    v290_.treasure.repairmap:InvokeServer()
    wait(1)
    v_u_287_ = false
end
function TeleportToTreasure()
    local v291_, v292_, v293_ = v_u_285_.PlayerGui["Treasure Map"].Main.CoordinatesLabel.Text:match("X ([%d.-]+), Y ([%d.-]+), Z ([%d.-]+)")
    if v291_ and (v292_ and v293_) then
        local v294_ = Vector3.new(tonumber(v291_), tonumber(v292_), tonumber(v293_))
        TeleportPlayer(CFrame.new(v294_))
    else
        warn("Failed to parse coordinates from text.")
    end
end
function ActivateNearestChestPrompt()
    local v295_ = math.huge
    local v296_ = v_u_285_.Character.PrimaryPart.Position
    local v297_, v298_, v299_ = pairs(workspace.world.chests:GetChildren())
    local v300_ = nil
    while true do
        local v301_
        v299_, v301_ = v297_(v298_, v299_)
        if v299_ == nil then
            break
        end
        if v301_:FindFirstChild("ProximityPrompt") then
            local v302_ = (v301_.Position - v296_).Magnitude
            if v302_ < v295_ then
                v300_ = v301_.ProximityPrompt
                v295_ = v302_
            end
        end
    end
    if v300_ then
        fireproximityprompt(v300_, 1, true)
    else
        warn("No Treasure Chest ProximityPrompt found.")
    end
end
local v303_ = v61_.NPC:AddSection("[ Merlin ]")
