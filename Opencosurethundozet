local v_u_103_ = game:GetService("Players").LocalPlayer
local function v_u_105_()
    if not v_u_103_.Character then
        return false
    end
    local v104_ = v_u_103_.Character:FindFirstChildWhichIsA("Tool")
    if v104_ then
        v104_ = v_u_103_.Character:FindFirstChildWhichIsA("Tool").Name:lower():find("rod")
    end
    return v104_
end
local function v_u_111_()
    local v106_ = v_u_103_.Backpack
    local v107_, v108_, v109_ = ipairs(v106_:GetChildren())
    while true do
        local v110_
        v109_, v110_ = v107_(v108_, v109_)
        if v109_ == nil then
            break
        end
        if v110_:IsA("Tool") and string.find(v110_.Name:lower(), "rod", 1, true) then
            if v_u_103_.Character and v_u_103_.Character:FindFirstChild(v110_.Name) == nil then
                v_u_103_.Character.Humanoid:EquipTool(v110_)
            end
            return v110_
        end
    end
    return nil
end
function EquipItem(p_u_112_)
    pcall(function()
        if game.Players.LocalPlayer.Backpack:FindFirstChild(p_u_112_) then
            local v113_ = game.Players.LocalPlayer.Backpack:FindFirstChild(p_u_112_)
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(v113_)
        end
    end)
end
