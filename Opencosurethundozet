function getHeldFish()
    local v254_, v255_, v256_ = ipairs(game.Players.LocalPlayer.Character:GetChildren())
    while true do
        local v257_
        v256_, v257_ = v254_(v255_, v256_)
        if v256_ == nil then
            break
        end
        if v257_:IsA("Tool") then
            return v257_
        end
    end
    return nil
end
function switchFish()
    local v258_, v259_, v260_ = ipairs(game.Players.LocalPlayer.Backpack:GetChildren())
    while true do
        local v261_
        v260_, v261_ = v258_(v259_, v260_)
        if v260_ == nil then
            break
        end
        if table.find(v_u_228_.Value, v261_.Name) and not table.find(v_u_211_, v261_) then
            print("Equipping:", v261_.Name)
            game.Players.LocalPlayer.PlayerGui.hud.safezone.backpack.events.equip:FireServer(v261_)
            return
        end
    end
end
function getWeightCategory(p262_)
    local v263_ = p262_.Name
    local v264_ = p262_.link.Value.Weight.Value
    local v265_ = v_u_209_[v263_] / 10
    return v_u_209_[v263_] / 10 * 1.99 >= v264_ and (v265_ < v264_ and "Big" or "Regular") or "Giant"
end
game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(p266_)
    if p266_:isA("Tool") and not p266_:FindFirstChild("link") then
        repeat
            task.wait()
        until p266_:FindFirstChild("link")
        local v267_ = getHeldFish()
        if v267_ then
            v267_.Parent = game.Players.LocalPlayer.Backpack
        end
        if v_u_236_.Value then
            game.Players.LocalPlayer.PlayerGui.hud.safezone.backpack.events.equip:FireServer(p266_)
        end
    end
end)
function applyFilter(p268_)
    local v269_ = p268_.link.Value
    if v_u_246_.Value and (next(v_u_241_) ~= nil and not table.find(v_u_241_, getWeightCategory(p268_))) then
        return false
    end
    if v_u_250_.Value and not v269_:FindFirstChild("Sparkling") then
        return false
    end
    if v_u_252_.Value and not v269_:FindFirstChild("Shiny") then
        return false
    end
    if not v_u_248_.Value or next(v_u_222_) == nil then
        return true
    end
    local v270_ = nil
    if v269_ then
        v270_ = v269_:FindFirstChild("Mutation")
    else
        print("Stat Folder Mutation not Found")
    end
    local v271_ = table.find(v_u_222_, "Any")
    if not v270_ then
        return v271_ and true or false
    end
    if v271_ then
        return true
    end
    local v272_, v273_, v274_ = ipairs(v_u_222_)
    while true do
        local v275_
        v274_, v275_ = v272_(v273_, v274_)
        if v274_ == nil then
            break
        end
        if v275_ == v270_.Value then
            return true
        end
    end
    return false
end
function AutoAppraise()
    if v_u_236_.Value then
        local v276_ = getHeldFish()
        if v276_ then
            if applyFilter(v276_) then
                if v_u_238_.Value and not table.find(v_u_211_, v276_) then
                    table.insert(v_u_211_, v276_)
                    switchFish()
                end
                return
            end
            local v277_, v278_ = pcall(function()
                workspace.world.npcs.Appraiser.appraiser.appraise:InvokeServer()
            end)
            if not v277_ then
                print("Error appraising fish:", v278_)
            end
            task.wait(tonumber(v_u_210_))
        elseif not v276_ and v_u_238_.Value then
            switchFish()
        end
    end
end
v101_.Heartbeat:Connect(AutoAppraise)
